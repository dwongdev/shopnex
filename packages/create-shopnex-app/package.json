{
    "name": "create-shopnex-app",
    "version": "3.56.2",
    "homepage": "https://shopnex.ai",
    "repository": {
        "type": "git",
        "url": "https://github.com/shopnex/shopnex.git",
        "directory": "packages/create-shopnex-app"
    },
    "license": "MIT",
    "author": "ShopNex <dev@shopnex.ai> (https://shopnex.ai)",
    "maintainers": [
        {
            "name": "ShopNex",
            "email": "info@shopnex.ai",
            "url": "https://shopnex.ai"
        }
    ],
    "sideEffects": false,
    "type": "module",
    "exports": {
        "./types": {
            "import": "./src/types.ts",
            "require": "./src/types.ts"
        },
        "./commands": {
            "import": "./src/lib/init-next.ts",
            "require": "./src/lib/init-next.ts",
            "types": "./src/lib/init-next.ts"
        },
        "./lib/*": {
            "import": "./src/lib/*",
            "require": "./src/lib/*",
            "types": "./src/lib/*"
        },
        "./utils/*": {
            "import": "./src/utils/*",
            "require": "./src/utils/*",
            "types": "./src/utils/*"
        }
    },
    "bin": {
        "create-shopnex-app": "bin/cli.js"
    },
    "files": [
        "package.json",
        "dist",
        "bin"
    ],
    "scripts": {
        "build": "pnpm pack-template-files && pnpm typecheck && pnpm build:swc",
        "build:swc": "swc ./src -d ./dist --config-file .swcrc --strip-leading-paths",
        "clean": "rimraf -g {dist,*.tsbuildinfo}",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "pack-template-files": "node --no-deprecation --import @swc-node/register/esm-register src/scripts/pack-template-files.ts",
        "prepublishOnly": "pnpm clean && pnpm build",
        "start": "node --no-deprecation --import @swc-node/register/esm-register src/start.js",
        "test": "jest",
        "test:e2e": "node --no-deprecation --import @swc-node/register/esm-register src/scripts/test-e2e.ts",
        "test:simple": "node --no-deprecation --import @swc-node/register/esm-register src/scripts/test-simple.ts",
        "typecheck": "tsc"
    },
    "dependencies": {
        "@clack/prompts": "^0.7.0",
        "@sindresorhus/slugify": "^1.1.0",
        "@swc/core": "1.13.5",
        "arg": "^5.0.0",
        "chalk": "^4.1.0",
        "comment-json": "^4.2.3",
        "esprima-next": "^6.0.3",
        "execa": "^5.0.0",
        "figures": "^6.1.0",
        "fs-extra": "^9.0.1",
        "globby": "11.1.0",
        "tar": "^7.4.3",
        "terminal-link": "^2.1.1"
    },
    "devDependencies": {
        "@jest/globals": "^30.1.2",
        "@swc-node/register": "^1.11.1",
        "@swc/jest": "^0.2.39",
        "@types/esprima": "^4.0.6",
        "@types/fs-extra": "^9.0.12",
        "@types/jest": "29.5.12",
        "@types/node": "22.15.30",
        "jest": "^30.1.3"
    },
    "engines": {
        "node": "^18.20.2 || >=20.9.0"
    }
}